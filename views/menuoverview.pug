docttpe html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        link(rel="stylesheet" href="/css/showmenuoverview.css")
        link(rel="stylesheet" href="https://fonts.googleapis.com/css?family=Ribeye")
        title Menuoverview
    body
      .topnav
        a(href='/about') About FoodHippO
        a(href='/showweeklymenus') WeeklyMenusüç±
        a(href='/searchweeklymenu') Search By Location
        a(href="/") Home
      .wrapper
        each menu in menus
          .card
            a.wholebutton(href=`/menuoverview/${menu.servicename}`)
            img(src=`../uploads/${menu.avatar}`)
            .info
              p=`${menu.servicename}`
              p=`${menu.area}`
              p=`${menu.typeoffood}`
              p=`${menu.processingtime}`









